{
  "name": "@droppii-org/ui",
  "version": "0.0.1",
  "description": "Droppii React UI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./antd-components/*": {
      "import": "./dist/antd-components/*.js",
      "types": "./dist/antd-components/*.d.ts"
    },
    "./antd-form/*": {
      "import": "./dist/antd-form/*.js",
      "types": "./dist/antd-form/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@reach/auto-id": "^0.18.0",
    "antd": "^5.27.0"
  }
}
